interactive program {
  INIT     -> { lmc := miLmc()             }
  K_1      -> { MostrarLMCInicial(miLmc()) }
  K_RETURN -> { EjecutarLMC(miLmc())       }
}

procedure MostrarLMCInicial(lmc) { PonerLMC_(lmc)                  }
procedure EjecutarLMC(lmc)       { PonerLMC_(execute_Program(lmc)) }

function miPrograma() { return ([901, 902]) }
function miInput()    { return ([10, 20, 30]) }
function miLmc()      { return (loadProgram_On_(miPrograma(),
                                loadInput_On_(miInput(), 
                                emptyLMCWithMemSize_(20)
                               ))) 
                      }


